             xmlns:local="clr-namespace:wpfreg"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source=" ModernButton.xaml"/>
